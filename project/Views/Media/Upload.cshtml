@model project.Models.MediaElement

@{
    ViewBag.Title = "Upload";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Upload New Media</h2>
@using (Html.BeginForm("UploadFile", "Media", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="editor-label">
        @Html.LabelFor(model => model.Title)
    </div>
    <div class="editor-field">
        @Html.EditorFor(model => model.Title)
        @Html.ValidationMessageFor(model => model.Title)
    </div>

    <div class="editor-label">
        @Html.LabelFor(model => model.IsPublic)
    </div>
    <div class="editor-field">
        @Html.EditorFor(model => model.IsPublic)
        @Html.ValidationMessageFor(model => model.IsPublic)
    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.Genre)
    </div>  
    /*<div class="editor-field">
        @Html.DropDownListFor(model => model.Genre, model.RateItem);
    </div>*/
    
    <span>
        <input type="file" name="myFile" accept=".mp3" /> <br>
        <input type="submit" value="Upload" />
    </span>
}
<aside>
    <h3>Uploaded songs</h3>
    <h1>songs</h1>
    <p>Here will all the songs that you have uploaded appear.</p>
</aside>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>
@section Scripts {
    <script src="~/Scripts/media-upload.js"></script>
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")
}